{
  "name": "vaillovehunt",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "vite build",
    "preview": "vite preview",
    "server": "tsx src/server/server.ts",
    "server:dev": "tsx --watch src/server/server.ts",
    "state-server": "node src/server/stateServer.js",
    "state-server:dev": "nodemon src/server/stateServer.js",
    "start": "concurrently --prefix-colors \"cyan,magenta\" --prefix \"[UI],[API]\" \"npm run dev\" \"npm run server:dev\"",
    "start:full": "concurrently --prefix-colors \"cyan,magenta,yellow\" --prefix \"[UI],[API],[STATE]\" \"npm run dev\" \"npm run server:dev\" \"npm run state-server:dev\"",
    "start:netlify": "netlify dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@netlify/blobs": "^10.0.10",
    "@tanstack/react-query": "^5.89.0",
    "cloudinary": "^2.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "parse-multipart-data": "^1.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "streamifier": "^0.1.1",
    "swr": "^2.3.6",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@netlify/functions": "^4.2.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.0",
    "vitest": "^3.2.4"
  }
}
